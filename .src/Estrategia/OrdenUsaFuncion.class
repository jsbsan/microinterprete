Inherits InterfaceCrearComando
Private comandotmpLinea As New UsaFuncion

Public Sub guard(valor As String) As Boolean

    If valor = ("usafuncion") Then
        Return True
    Else
        Return False
    Endif

End

Public Sub crearComando(linea As String) As Comando

    Dim palabras As String[]

    palabras = Split(linea, ",") 'caracter separador de datos

    If palabras.count = 0 Then
        Return Null
    Else
        comandotmpLinea = New UsaFuncion
        comandotmpLinea.datos = palabras
        comandotmpLinea.tipo = "interno"
        Return comandotmpLinea
    Endif

End

Public Sub ayuda() As String

    'devuelve lo ayuda sobre el comando

    Return "Usa la funcion dada. Sintaxis: usafuncion,[nombre],[valor de parametros separados por ;]. Si queremos leer el valor de retorno, la variable se llama: [nombrefuncion]_retorno \n Ejemplo: usafuncion,doblerectangulo,10;10;20;20"

End
